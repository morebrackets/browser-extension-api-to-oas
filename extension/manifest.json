{
  "manifest_version": 3,
  "name": "API to OAS",
  "version": "1.0",
  "description": "Captures API requests and generates an OpenAPI Specification.",
  "permissions": [
    "storage",
    "activeTab",
    "debugger"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
